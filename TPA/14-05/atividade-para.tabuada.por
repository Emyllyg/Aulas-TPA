programa
{
	
	funcao inicio()
	{	 
		caracter resp

		faca{
		
			limpa()

			escreva("Tabuada do número 9\n")
			
			para(inteiro i=1; i<=10; i++ /*incremento*/ ){
			escreva(9,"x",i,"=",9*i, "\n")
		}
		
			escreva("Executar novamente? (S/N): ")
			leia(resp)
		} enquanto(resp == 'S')
	}
}
